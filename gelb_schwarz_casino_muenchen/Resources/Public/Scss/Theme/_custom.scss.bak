/* Paginierung */
  .pagination {
    display: unset;
  }

  .pagination li {
    display: inline;
    margin-right: 7px;
  }

  .pagination li a {
    color: #ff8700 !important;
  }

/* Galerie */
  .c-gallery .figure {
    margin: 0 5px 0;
  }

  .gallerylist-outer { 
    display: flex; 
    justify-content:space-between; 
  }
  
  .gallerylist-inner { 
    width: auto; 
  }

  .gallerylist-right {
    width: 220px;
    float: right;
    padding-top: 0.8rem;
  }

  .gallery-partial { 
    -webkit-column-count: 3;
    -moz-column-count: 3;
    column-count: 3;
    -webkit-column-gap: 10px;
    -moz-column-gap: 10px;
    column-gap: 10px;
    margin-top: 10px;
    overflow: hidden;
  }

  .c-gallery img { 
    //max-width: 320px; 
    height: auto;
    transition: 500ms;
    margin-bottom: 10px;
    opacity: 0.8;
    page-break-inside: avoid; 
    -webkit-column-break-inside: avoid; 
    break-inside: avoid; 
  }

  .c-gallery img:hover {
    opacity: 1;
  }

  .c-gallery__description, .c-gallery__location, c-gallery__date { 
    letter-spacing: 0.04em;
    font-size: clamp(13px, calc(0.8125rem +(18 - 13)*((100vw - 300px) /(1600 - 300))), 18px);
    line-height: clamp(20px, calc(1.25rem +(28 - 20)*((100vw - 300px) /(1600 - 300))), 28px);
  }

  .gallery-title {
    border: solid #556066 1px;
    width: 89%;
    margin-bottom: 10px;
    margin-left: 20px;
  }

  .gallery-back {
    padding-left: 15%;
    padding-top: 20px;
  }

  .gallery-backbtn {
    -webkit-appearance: button;
    cursor: pointer;
    color: #ffffff;
    background-color: #ffc300;
    border-color: #ffc300;
    padding: 6px 12px;
    border: 1px solid;
  }

  .filter {
    position: absolute;
    width: 100%;
    height: 100%;
    filter: blur(20px);
    opacity: 0.5;
    background-position: center;
