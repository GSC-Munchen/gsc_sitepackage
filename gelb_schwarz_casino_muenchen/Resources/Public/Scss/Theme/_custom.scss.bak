/****************************************************/
/* Default GSC München styles for bootstrap_package */
/* Letzte Änderung: 23.11.2025                      */
/****************************************************/

/* Allgemeines */
  body { 
    font-family: 'SourceSansPro-Light';
  } 
  
  a {
    color: #ff8700;
    text-decoration: none;
  }
  
  p a {
    color: #000000;
    font-family: 'SourceSansPro-Light';
    text-decoration: none;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
  }
  
  p a:hover {
    font-family: 'SourceSansPro-Light';
    color: #ffc300 !important;
    text-decoration: none;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
  }

  .frame-container, .container {
    padding-left: 0px;
    padding-right: 0px;
  }

  .frame { // Letzte Änderung
    padding-top: 0px;
  }

/* Navbar */
  .navbar-default { 
    background-color: #000000 !important;
  }
  
  .navbar-default .container {
    max-width: 980px;
    margin: auto;
  }

  .nav-link { 
    color: #FFFFFF !important;
    font-size: clamp(14px, calc(0.875rem + (19 - 14) * ((100vw - 300px) / (1600 - 300))), 19px); // 14px-19px
  }
  
  .nav-link.show { 
    color: #FFFFFF !important;
  } 
  
  .nav-link.visited { 
    color: #FFFFFF !important;
  } 
  
  .navbar-toggler { 
    color: #FFFFFF !important;
  } 
  
  .dropdown-nav .nav-link {
    color: #000000 !important;
  } 
  
  .dropdown-nav .nav-link:hover {
    background: #feeaa5 !important;
    color: #000000 !important;
  } 
  
  .dropdown-menu {
    background-color: rgb(255,195,0) !important;
    background: url('../../Images/Package/dropdown_background.png') repeat;
  }
  
  .dropdown-text {
    color: #000000 !important;
    font-family: 'SourceSansPro-Light';
    font-size: clamp(14px, calc(0.875rem + (19 - 14) * ((100vw - 300px) / (1600 - 300))), 19px); // 14px-19px, war 1.2em, neue Berechnung
  }


/* Breadcrumb Menu */
  .breadcrumb-section .container {
    max-width: 980px;
    margin: auto;
  }

  .breadcrumb-section {
    background-color: #FFD55F;
    position: fixed;
    width: 100%;
    z-index: 10;
    color: #000000;
    font-size: 1.2em;
    font-family: 'SourceSansPro-Light';
  } 
  
  .breadcrumb-section a {
    font-family: 'SourceSansPro-Regular';
    text-decoration: none;
    color: #000000;
  } 
  
  .breadcrumb-section a:hover {
    color: #FFFFFF;
  } 

/* Ueberschriften */
  h1 {
    font-family: 'SourceSansPro-SemiBold';
    color: rgb(255,195,0);
    font-size: clamp(26px, calc(1.625rem + (36 - 26) * ((100vw - 300px) / (1600 - 300))), 36px); // 26px-36px, war 2.25em
    padding-bottom: 14px;
    margin-bottom: 0px;
    margin-top: 20px;
  }
  h2 {
    font-family: 'SourceSansPro-SemiBold';
    color: rgb(85,96,102);
    font-size: clamp(19px, calc(1.1875rem + (26 - 19) * ((100vw - 300px) / (1600 - 300))), 26px); // 19px-26px, war 1.63em
    padding-bottom: 14px;
    margin-bottom: 0px;
    margin-top: 20px;
  }
  h3 {
    font-family: 'Bitter-Regular';
    color: rgb(85,96,102);
    margin-bottom: 0px;
    margin-top: 14px;
    font-size: clamp(13px,calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))),18px);
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    letter-spacing: 0.04em;
  }
  h4 {
    font-family: 'SourceSansPro-Regular';
    color: rgb(85,96,102);
    font-size: clamp(14px, calc(0.875rem + (19 - 14) * ((100vw - 300px) / (1600 - 300))), 19px); // 14px-19px, war 1.2em
    margin-bottom: 0px; 
    margin-top: 9px;
    line-height: clamp(18px, calc(1.125rem + (24 - 18) * ((100vw - 300px) / (1600 - 300))), 24px); // 18px-24px, war 1.5em 
  }
  h5 {
    font-family: 'Bitter-Italic';
    color: rgb(0,0,0);
    margin-top: 0px;
    margin-bottom: 0px;
    font-size: clamp(13px,calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))),18px);
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    letter-spacing: 0.04em;
  }
  h6 {
    font-family: 'SourceSansPro-Regular';
    color: rgb(85,96,102);
    font-size: clamp(14px, calc(0.875rem + (19 - 14) * ((100vw - 300px) / (1600 - 300))), 19px); // 14px-19px, war 1.2em
    margin-top: 0px;
    margin-bottom: 30px;
    line-height: clamp(18px, calc(1.125rem + (24 - 18) * ((100vw - 300px) / (1600 - 300))), 24px); // 18px-24px, war 1.5em 
  }
  
/* Text */
  p {
    margin: 0 0 10px;
    font-family: 'SourceSansPro-Light';
    color: #000000;
    margin-bottom: 14px;
    letter-spacing: 0.04em;
    font-size: clamp(13px, calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))), 18px); // 13px-18px, war 1.13em
    line-height: clamp(20px, calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))), 28px);  // 20px-28px, war 1.75em
  }
  
/* Links in Überschriften */
  h3 a {
    font-family: 'Bitter-Regular';
    color: #556066;
    margin-bottom: 0px;
    margin-top: 14px;
    text-decoration: none;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
    font-size: clamp(13px,calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))),18px);
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    letter-spacing: 0.04em;
  }
  
  h3 a:hover {
    text-decoration: none;
    color: #ffc300 !important;
  }
  
  h5 a {
    font-family: 'Bitter-Italic';
    color: #000000;
    margin-top: 0px;
    margin-bottom: 0px;
    text-decoration: none;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
  }
  
  h5 a:hover {
    color: #ffc300 !important;
    text-decoration: none;
  }

/* Tabellen */
  table {
    border-spacing: 0px 4px;
    border-collapse: separate;
    width: 100%;
    background-color: transparent;
  }

  .table td {
    background-color: #feeaa5;
    height: 40px;
    border-width: 0px;
    display: table-cell;
    padding: 0.75rem;
  }

  table tr>:first-child {
    font-family: 'Bitter-Regular';
    font-size: clamp(13px,calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))),18px);
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    padding-left: 10px;
  }

  .tr-content {
    height: 40px;
    background-color: #feeaa5;
  }

  .section-default tr a {
    font-family: 'SourceSansPro-Light';
    color: #000000 !important;
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    text-decoration: none;
    padding-right: 35px;
    background: url('Images/tr-linkpfeil.png') no-repeat center right;
}

/* Container und Layouts */
/* Allgemein */
.section-default  {
    max-width: 980px;
    margin: auto;
    padding-top: 85px;
  } /* */
  
  /* Abstand unten "extra large" auf 80px */
  .frame-space-after-extra-large {
    margin-bottom: 80px;
  }

/* Footer */
  .footer-section-meta {
    background-color: #556066;
    text-align: unset;
  }

  .footer-menu-section+.footer-info-section {
    margin-top: 0px; 
  }

  .footer-section .frame-container-default {
    max-width: unset; // zieht den schwarzen Streifen auf 100% der Seite
    width: 100%;
  }

  .footer-section .frame {
    padding-top: 0px;
    padding-bottom: 0px;
  }

  .footer-section-meta p {
    min-height: 80px;
    font-family: 'SourceSansPro-Regular';
    font-size: clamp(12px, calc(0.75rem + (16 - 12) * ((100vw - 300px) / (1600 - 300))), 16px);  // 12px-16px, war 1em
    color: #FFFFFF;
    line-height: 1.42857143;
  }
  
  .footer-meta {
    background: #000000;
    height: 40px;
    padding-top: 5px;
  }

  .footer-section-meta td {
    font-size: clamp(12px, calc(0.75rem + (16 - 12) * ((100vw - 300px) / (1600 - 300))), 16px);  // 12px-16px, war 1em
    font-family: 'SourceSansPro-Regular';
  }
  
  .footer-section-meta a:not(.btn) {
    color: #FFFFFF;
    text-decoration: none;
    font-family: 'SourceSansPro-Regular';
    font-size: clamp(15px, calc(0.9375rem + (21 - 15) * ((100vw - 300px) / (1600 - 300))), 21px);  // 15px-21px, war 1.31em
  }

  .footer-section-meta a:hover {
    color: #ffc300 !important;
  }

  .footer-copyright {
    max-width: 980px;
    margin: auto;
  }

  .footer-copyright table {
    text-align: left;
    // border-spacing: 20px 4px;
    padding-bottom: 10px;
    // border-collapse: unset;
  }

  .footer-copyright td {
    padding-right: 30px;
    font-size: clamp(12px,calc(0.75rem + (16 - 12) * ((100vw - 300px) / (1600 - 300))),16px) !important;
    font-family: "SourceSansPro-Regular" !important;
    padding-left: 0px !important;
  }

  .meta-menu-table {
    width: 100%;
  }

  .meta-spacer {
    width: 50%;
  }
  
/* Rahmen */
  .frame-framebox1 {
    border-top: 2px dotted grey;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
    margin-bottom: 60px;
    padding-bottom: 5px;
}

/* News */
  .news h3 a {
    color: #ffc300 !important;
    font-family: 'Bitter-Regular';
    margin-bottom: 0px;
    margin-top: 14px;
    text-decoration: none;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
    font-size: clamp(13px,calc(0.8125rem + (18 - 13) * ((100vw - 300px) / (1600 - 300))),18px);
    line-height: clamp(20px,calc(1.25rem + (28 - 20) * ((100vw - 300px) / (1600 - 300))),28px);
    letter-spacing: 0.04em;
  }

  .news .articletype-0 {
    margin-bottom: 60px;
    padding-bottom: 15px;
    border-top: 2px dotted gray;
    border-bottom: 2px dotted gray;
  }
  
  .news .news-list-item:first-of-type {
    margin-top: 15px        // Abstand zur Paginierung
  }

  .news .extra {
    color: #ffc300;
    font-family: 'SourceSansPro-Regular';
    font-size: clamp(12px,calc(0.75rem + (16 - 12) * ((100vw - 300px) / (1600 - 300))),16px);
    padding-top: 5px;
    margin-top: 5px;
    border-top: 1px solid #eee;
  }

  .news-single .extra {
    border-top: 2px dotted grey;
    border-bottom: 2px dotted grey;
    border-radius: 1px;
    padding-bottom: 5px;
    max-width: 960px;
  }

  .news-single img {
    vertical-align: unset;
  }

/* Startseite: News Inhalte standardmäßig ausblenden. Unten bei den Screenbreiteneinstellungen wird es bei Handys einglendet */
  #Startseite .section-light {
    display: none;
  }

  /* Startseite: Ausblenden der Breadcrumb Navigation */
  .backendlayout-special_start .breadcrumb-section {
    display: none;
  }

/* neuer Slider auf der Startseite */
  #Startseite .section-default {
    padding-top: 0px;
  }

  .slick-image img {
    max-height: 46vw;
    max-width: fit-content;
  }
  // CSS Weiche für Firefox, damit das Sliderbild auf der Startseite nicht verzerrt
  @-moz-document url-prefix() {
    .slick-image img {
      max-width:min-content;
      height: auto !important;
      width: auto !important;
    }
  }

  .slick-logo {
    position: absolute;
    top: 130px;
    right: 160px;
    z-index: 100;
  }

  .slick-lines {
    position: absolute;
    display: block;
    z-index: 50;
    top: 24%;
  }
  
  .slick-lines img {
    max-height: 23vw;
    max-width: fit-content;
  }
  // CSS Weiche für Firefox, damit das Sliderbild auf der Startseite nicht verzerrt
  @-moz-document url-prefix() {
    .slick-lines img {
      max-width:min-content;
      height: auto !important;
      width: auto !important;
    }
  }

  .slick-text {
    position: absolute;
    display: block;
    z-index: 55;
    top: 24%;
    left: 15vw;
  }
  
  .slick-text img {
    max-height: 23vw;
    max-width: fit-content;
  }
  // CSS Weiche für Firefox, damit das Sliderbild auf der Startseite nicht verzerrt
  @-moz-document url-prefix() {
    .slick-text img {
      max-width:min-content;
      height: auto !important;
      width: auto !important;
    }
  }


  .slick-zoom {
    animation: scale 30000ms linear infinite;
  }
    
  @keyframes scale {
    50% {
      -webkit-transform:scale(1.1);
      -moz-transform:scale(1.1);
      -ms-transform:scale(1.1);
      -o-transform:scale(1.1);
      transform:scale(1.1);
    }
  }

  .slick-prev, .slick-next, .slick-arrow {
    border-radius: 0 3px 3px 0;
    background-color: #556066 !important;
    z-index: 100;
    width: 50px !important;
    height: 30px !important;
  }

  .slick-prev {
    left: 0px !important;
  }

  .slick-next {
    right: 0px !important;
  }

